type Option = Some (i32, i32, i32, i32) | None

fn passes v as Option -> Option = v

fn returns a as (i32, i32, i32, i32) -> Option =
  Some a

fn exits a as Option -> () =
  match a
  | Some (a, b, c, d) -> libc_exit (a + b + c + d)
  | None   -> libc_exit 100

fn main =
  exits (passes (returns (1, 2, 3, 4)))
